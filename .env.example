# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# n8n Webhook Configuration
# After importing n8n Workflow 1, copy the webhook URL here
VITE_N8N_INITIAL_MESSAGE_WEBHOOK="https://your-n8n-instance.com/webhook/initial-message"

# Generate a secure random token (e.g., using: openssl rand -hex 32)
# This token must match the N8N_WEBHOOK_TOKEN environment variable in n8n
VITE_N8N_WEBHOOK_TOKEN="your-secure-token-here"
